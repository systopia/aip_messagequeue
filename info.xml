<?xml version="1.0"?>
<extension key="aip_messagequeue" type="module">
  <file>aip_messagequeue</file>
  <name>aip_messagequeue</name>
  <description>Extends  automated import processors  for importing of message queues</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Johannes</author>
    <email>margraf@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/aip_messagequeue</url>
    <url desc="Documentation">https://github.com/systopia/aip_messagequeue</url>
    <url desc="Support">https://github.com/systopia/aip_messagequeue</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-04-16</releaseDate>
  <version>0.2</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.79</ver>
  </compatibility>
  <comments>Extends  automated import processors  for importing of message queues</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/AipMessagequeue</namespace>
    <format>23.02.1</format>
    <angularModule>crmAipMessagequeue</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
